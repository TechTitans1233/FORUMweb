{
  "name": "forum-web",
  "version": "1.0.0",
  "type": "module",
  "description": "Forum web application",
  "main": "server.js",
  "dependencies": {
    "@faker-js/faker": "^9.8.0",
    "@raruto/leaflet-elevation": "^2.5.1",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "firebase": "^11.7.3",
    "multer": "^1.4.5-lts.2",
    "firebase-admin": "^13.4.0",
    "geckodriver": "^5.0.0",
    "leaflet-topography": "^0.2.1",
    "ngeohash": "^0.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "babel-jest": "^29.7.0",
    "chromedriver": "^136.0.2",
    "coveralls": "^3.1.0",
    "generator-jest": "^1.8.0",
    "glob": "^10.4.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.1.0",
    "live-server": "^1.2.2",
    "nightwatch": "^3.12.1",
    "nodemon": "^3.1.10",
    "selenium-webdriver": "^4.33.0",
    "supertest": "^7.1.1",
    "util": "^0.12.5",
    "yo": "^5.1.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test:e2e": "nightwatch --config nightwatch.docker.conf.cjs",
    "test:e2e:local": "nightwatch --config nightwatch.conf.cjs",
    "test:unit": "jest --config=jest.unit.config.js",
    "test:int": "jest --config=jest.int.config.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:test": "docker-compose --profile testing up --build --abort-on-container-exit e2e-tests",
    "docker:logs": "docker-compose logs -f",
    "test": "jest",
    "generate:tests": "yo jest",
    "posttest": "cat ./coverage/lcov.info | coveralls",
    "generate:specs": "node scripts/generate-specs.js",
    "serve": "live-server"
  },
  "engines": {
    "node": ">=22.11.0",
    "npm": ">=10.9.0"
  }
}
